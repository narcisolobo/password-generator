{
    "manifest_version": 2,
    "name": "Password Generator",
    "version": "1.0",
    "homepage_url": "https://github.com/narcisolobo/password-generator",
    "description": "Generates a password like the ones generated by Safari.",
    "icons": {
      "48": "icons/key-48.png"
    },

    "browser_action": {
      "default_icon": "icons/key-32.svg",
      "default_title": "Safari Password Generator",
      "default_popup": "popup/index.html"
    }
  }
